{
    "name": "mybb/settings",
    "description": "MyBB 2.0 settings package.",
    "license": "BSD-3",
    "authors": [
        {
            "name": "Euan T.",
            "email": "euantor@mybb.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^5.0",
        "illuminate/config": "^5.0",
        "illuminate/auth": "^5.0",
        "illuminate/database": "^5.0",
        "illuminate/cache": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "MyBB\\Settings\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist"
    }
}
